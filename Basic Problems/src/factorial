    //1, 1, 2, 6, 24, 120, 720, 5040, 40320, 362880, 3628800 (0! = 1)
    public static void main(String[] args) {
        //factorial series
        int n = 10;
        for(int i=0; i<n;i++){
            System.out.print(factorial(i) + " ");
        }
    }

    private static int factorial(int n) {
        if(n<=1) return 1;
        else return n*factorial(n-1);
    }